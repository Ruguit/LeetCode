
class Solution {
    public int[] twoSum(int[] nums, int target) {
        int n=nums.length;
        for (int i = 0; i < n; ++i) {
            for (int i1 = i+1; i1 < n; ++i1) {
                if (target==nums[i]+nums[i1]) {
                    return new int[]{i,i1};
                }
            }
        }
        return new int[0];
    }
}
